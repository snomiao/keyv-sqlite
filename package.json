{
  "name": "@snomiao/keyv-sqlite",
  "version": "5.0.1",
  "description": "A new SQLite storage adapter for Keyv.",
  "keywords": [
    "keyv",
    "sqlite",
    "node:sqlite"
  ],
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "author": "Huijie Wei",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "bench": "vitest bench",
    "test": "vitest --ui",
    "check": "oxlint src && oxfmt src",
    "format": "oxfmt --write src"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@keyv/test-suite": "^2.0.2",
    "@types/node": "^25.1.0",
    "@vitest/ui": "^2.1.4",
    "keyv": "^5.2.1",
    "oxfmt": "^0.14.0",
    "oxlint": "^1.39.0",
    "tsdown": "^0.20.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "peerDependencies": {
    "keyv": "^5.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/huijiewei/keyv-sqlite.git"
  },
  "engines": {
    "node": ">=22.5.0"
  }
}

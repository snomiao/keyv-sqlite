{
  "name": "@snomiao/keyv-sqlite",
  "version": "5.0.1",
  "description": "A new SQLite storage adapter for Keyv.",
  "keywords": [
    "keyv",
    "sqlite",
    "node:sqlite",
    "better-sqlite3",
    "bun:sqlite"
  ],
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "author": "Huijie Wei",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "bench": "vitest bench --run --config=vitest.bench.config.ts",
    "test": "vitest --ui",
    "check": "oxlint src && oxfmt src",
    "format": "oxfmt --write src"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@keyv/test-suite": "^2.0.2",
    "@types/node": "^25.1.0",
    "@vitest/ui": "^2.1.4",
    "keyv": "^5.2.1",
    "oxfmt": "^0.14.0",
    "oxlint": "^1.39.0",
    "tsdown": "^0.20.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "peerDependencies": {
    "better-sqlite3": "^11.0.0",
    "keyv": "^5.2.1",
    "sqlite3": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "better-sqlite3": {
      "optional": true
    },
    "sqlite3": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/huijiewei/keyv-sqlite.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

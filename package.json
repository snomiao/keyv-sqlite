{
  "name": "@snomiao/keyv-sqlite",
  "version": "5.0.3",
  "description": "Multi-driver SQLite storage adapter for Keyv with support for node:sqlite, bun:sqlite, and better-sqlite3.",
  "keywords": [
    "keyv",
    "sqlite",
    "node:sqlite",
    "better-sqlite3",
    "bun:sqlite"
  ],
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "author": "Huijie Wei",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "bench": "vitest bench --run --config=vitest.bench.config.ts",
    "test": "node --experimental-sqlite --no-warnings ./node_modules/.bin/vitest --ui",
    "check": "oxlint src && oxfmt src",
    "format": "oxfmt --write src",
    "example:node": "node --experimental-sqlite --no-warnings examples/index.ts",
    "example:bun": "bun examples/index.ts"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@keyv/test-suite": "^2.0.2",
    "@types/node": "^25.1.0",
    "@vitest/ui": "^2.1.4",
    "better-sqlite3": "^12.6.2",
    "keyv": "^5.2.1",
    "oxfmt": "^0.14.0",
    "oxlint": "^1.39.0",
    "tsdown": "^0.20.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.4"
  },
  "peerDependencies": {
    "better-sqlite3": "^11.0.0",
    "keyv": "^5.2.1",
    "sqlite3": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "better-sqlite3": {
      "optional": true
    },
    "sqlite3": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snomiao/keyv-sqlite.git"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
